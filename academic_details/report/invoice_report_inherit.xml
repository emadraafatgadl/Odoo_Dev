<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="external_layout_header_inherit_cr" inherit_id="web.external_layout_background">
        <xpath expr="//img[@t-if='company.logo']" position="replace">
            <div>
                <img src="data:image/jpeg;base64,"/>
            </div>
        </xpath>

        <xpath expr="//div[@class='float-left company_address']" position="replace">
            <link rel="stylesheet" type="text/css" href="/academic_details/static/src/css/website_style.css"/>
            <div class="right12">
                <p class="line-ht">GREEN SOUCE JANITORIAL</p>
                <p class="line-ht">2625 Middlefield Rd #664</p>
                <p class="line-ht">Palo Alto, California 94306 </p>
                <p class="line-ht">accounting@greensourcejanitorial.com</p>
                <p class="line-ht">(650)488-8100</p>
            </div>
        </xpath>
    </template>

    <template id="invoice_report_inherit" inherit_id="account.report_invoice_document">
<!--        <t t-call="academic_details.external_layout_header_inherit_cr"/>-->

        <xpath expr="//address[@t-field='o.partner_id']" position="before">
<!--            <t t-call="academic_details.external_layout_header_inherit_cr"/>-->
            <link rel="stylesheet" type="text/css" href="/academic_details/static/src/css/website_style.css"/>
            <div class=".topup">
            <h1>Jul 2021-Invoice</h1>
            <table class="table">
                <thead>
                  <tr>
                    <th>Date</th>
                    <th>Invoice#</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><p class="m-0" t-field="o.invoice_date"/></td>
                    <td><p class="m-0" t-field="o.name"/></td>
                  </tr>
                </tbody>
            </table>
            </div>
        </xpath>

        <xpath expr="//address[@t-field='o.partner_id']" position="replace">
            <table class="table">
                <thead>
                  <tr>
                    <th>Ship To</th>
                    <th>Bill To</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><p class="m-0" t-field="o.partner_id" t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' /></td>
                    <td><p class="m-0" t-field="o.partner_id" t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' /></td>
                  </tr>
                </tbody>
              </table>
            <table class="table">
                <thead>
                  <tr>
                    <th>Terms</th>
                    <th>PO#</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><span t-field="o.invoice_payment_term_id.note"/></td>
                    <td></td>
                  </tr>
                </tbody>
              </table>

        </xpath>
        <xpath expr="//div[@class='page']" position="replace">
          <t t-set="lines" t-value="o.invoice_line_ids.sorted(key=lambda l: (-l.sequence, l.date, l.move_name, -l.id), reverse=True)"/>

            <t t-foreach="lines" t-as="line">
            <table class="table">
                <thead>
                  <tr>
                    <th>Quantity</th>
                    <th>Item description</th>
                    <th>Rate</th>
                    <th>Amount</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><span t-field="line.quantity"/></td>
                    <td name="account_invoice_line_name"><span t-field="line.name" t-options="{'widget': 'text'}"/></td>
                    <td><span class="text-nowrap" t-field="line.price_unit"/></td>
                    <td><span class="text-nowrap" t-field="line.price_total"/></td>
                  </tr>
                    <td></td>
                    <td></td>
                    <td><dt>Total</dt></td>
                    <td><span class="text-nowrap" t-field="line.price_total"/></td>
                </tbody>
              </table>
            </t>

        </xpath>

    </template>
</odoo>